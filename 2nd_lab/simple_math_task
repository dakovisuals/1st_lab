#include <iostream>
#include <math.h>

int main()
{
    int A;
    int B;
    int C;
    std::cout << "Input A side ( all values should be in (0 ; 3*10^4) ): ";
    std::cin >> A; 
    std::cout << "Input B side: ";
    std::cin >> B;
    std::cout << "Input C side: ";
    std::cin >> C;

    int diag = 0;
    diag = pow(A, 2) + pow(B, 2) + pow(C, 2);
    std::cout << "The diagonal of a rectangular parallelepiped is: " << diag << std::endl;

    return 0;

}
